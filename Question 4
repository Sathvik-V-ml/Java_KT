interface Playable{
        public void play();
}
public class Guiter implements Playable{
        public void play(){
        System.out.println("Guiter is been played");
        }
        public static void main(String []args){
                Guiter guiter = new Guiter();
                Piano piano = new Piano();
                guiter.play();
                piano.play();
        }
        }

class Piano implements Playable{
        public void play(){
        System.out.println("Piano is been played");
        }
}
